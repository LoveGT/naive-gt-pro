{
	"root": true,
	"parserOptions": {
		"ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
	},
	"extends": ["plugin:vue/vue3-recommended", "plugin:prettier/recommended"],
	"rules": {
		"vue/valid-template-root": "off",
		"vue/no-multiple-template-root": "off",
		"vue/multi-word-component-names": [
			"error",
			{
				"ignores": ["index", "401", "404", "Login"]
			}
		]
	}
}
